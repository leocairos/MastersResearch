@article{Ratches:1997:AAT:265182.265191,
abstract = {This paper systematically reviews 10 years of research that$\backslash$nseveral Army Laboratories conducted in object recognition algorithms,$\backslash$nprocessors, and evaluation techniques. In the military, object$\backslash$nrecognition is applied to the discrimination of military targets,$\backslash$nranging from human-aided to autonomous operations, and is called$\backslash$nautomatic target recognition (ATR). The research described here has been$\backslash$nconcentrated in human-aided target recognition applications, but some$\backslash$nattention has been paid to automatic processes. Definitions and$\backslash$nperformance metrics that have been developed are described along with$\backslash$nperformance data showing the present state-of-the-art. The effects of$\backslash$nsignal-to-noise and clutter parameters are indicated in the data.$\backslash$nMultisensor fusion and model-based algorithms are discussed as the$\backslash$nlatest techniques under consideration by the military research$\backslash$ncommunity. The results demonstrate that useful performance can be$\backslash$nachieved, and tools are evolving to understand and improve the$\backslash$nperformance under real-world conditions. The referenced research$\backslash$nstrongly indicates the need for the development of image science, as$\backslash$ndescribed in the paper, to support the theoretical underpinnings of ATR},
