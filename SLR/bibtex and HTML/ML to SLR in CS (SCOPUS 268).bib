url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84886683579{\&}doi=10.1109{\%}2FTVCG.2013.126{\&}partnerID=40{\&}md5=16de1b7c1668758d7dba36f4cbd3fb49},
