@article{Borg:2014:RDS:2683115.2683134,
abstract = {Engineers in large-scale software development have to manage large$\backslash$namounts of information, spread across many artifacts. Several researchers$\backslash$nhave proposed expressing retrieval of trace links among artifacts,$\backslash$ni.e. trace recovery, as an Information Retrieval (IR) problem. The$\backslash$nobjective of this study is to produce a map of work on IR-based trace$\backslash$nrecovery, with a particular focus on previous evaluations and strength$\backslash$nof evidence. We conducted a systematic mapping of IR-based trace$\backslash$nrecovery. Of the 79 publications classified, a majority applied algebraic$\backslash$nIR models. While a set of studies on students indicate that IR-based$\backslash$ntrace recovery tools support certain work tasks, most previous studies$\backslash$ndo not go beyond reporting precision and recall of candidate trace$\backslash$nlinks from evaluations using datasets containing less than 500 artifacts.$\backslash$nOur review identified a need of industrial case studies. Furthermore,$\backslash$nwe conclude that the overall quality of reporting should be improved$\backslash$nregarding both context and tool details, measures reported, and use$\backslash$nof IR terminology. Finally, based on our empirical findings, we present$\backslash$nsuggestions on how to advance research on IR-based trace recovery.},
address = {Hingham, MA, USA},
