keywords = {.ML to SLR - 2017 (2018.01.07),2. IEEE 2017,Benchmark data set,[lsc]: excluded,deep learning,handcrafted features,remote sensing image,scene classification,unsupervised feature learning,zz.IEEE2018.01.20,zz.ML to SLR in CS - 2018.01.20,zz.inGroup,zz11 IEEE 2017.12.09,zz2. IEEE 2017},
